<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.btcbrunch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MPChart01">
        <activity
            android:name="com.example.btcbrunch.BitCoinVsStocksActivity"
            android:exported="true"
            android:label="BTC Brunch Commodity/Bitcoin Line"
            android:parentActivityName="com.example.btcbrunch.MainActivity" />


        <activity
            android:name="com.example.btcbrunch.BitCoinCandleStickActivity"
            android:exported="true"
            android:label="BTC Brunch Candlestick Chart"
            android:parentActivityName="com.example.btcbrunch.MainActivity" />
        <activity android:name="com.example.btcbrunch.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>